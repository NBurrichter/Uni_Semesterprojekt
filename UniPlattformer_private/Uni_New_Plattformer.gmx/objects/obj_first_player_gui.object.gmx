   then
   {
   move_towards_point(movex[onthemoveto],movey[onthemoveto],movespeed);
   }

//snapen zum Punkt
if point_distance(x,y,movex[onthemoveto],movey[onthemoveto]) &lt; (speed / 0.99)
   then
   {
   x = movex[onthemoveto];
   y = movey[onthemoveto];
   speed = 0;
   }

//Zähler fürs nächste/letzte level erhöhen bzw erniedrigen
if (x = movex[onthemoveto]) and (y = movey[onthemoveto])
    then
    {
    if onthemoveto &gt; moveto then
        {
        onthemoveto--;
        }
    if onthemoveto &lt; moveto then
        {
        onthemoveto++;
        }
      
    }
    
if keyboard_check_pressed(global.key_left) and !mouse_over
    then
    {
    if moveto &gt; 1
        moveto--;
    }
    
if keyboard_check_pressed(global.key_right) and !mouse_over
    then
    {
    if moveto &lt; 9
        if moveto &lt; global.level_save
        then
        {
            moveto++;
        }
    }
    

    
if vspeed &lt; 0{
