        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Save and return to options menu room

if mouse_over = true
    then
    {
    if room == room_option_controls
        then
        {
        global.key_up = keyboard_get_map(obj_key_up.chosen_key);
        global.key_down = keyboard_get_map(obj_key_down.chosen_key);
        global.key_left = keyboard_get_map(obj_key_left.chosen_key);
        global.key_right = keyboard_get_map(obj_key_right.chosen_key);
        
        ini_open("settings.ini");
        ini_write_real("controls", "up", keyboard_get_map(global.key_up));
        ini_write_real("controls", "down", keyboard_get_map(global.key_down));
        ini_write_real("controls", "left", keyboard_get_map(global.key_left));
        ini_write_real("controls", "right", keyboard_get_map(global.key_right));
        ini_close();     
        }
    else
    if room == room_option_video
        then
        {
        global.fullscreen = obj_key_fullscreen.fullscreen_enabled;
        
        ini_open("settings.ini");
        ini_write_real("video", "fullscreen", global.fullscreen);
        ini_close();   
        }
    else
    if room == room_option_audio
        then
        {
        global.mute = obj_key_mute.is_muted;
        
        ini_open("settings.ini");
        ini_write_real("audio", "mute", global.mute);   
        ini_close();
        }
    room_goto(room_option_menu);
    audio_play_sound (snd_select,1,false);
    }

    
</string>
